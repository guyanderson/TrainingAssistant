@model IEnumerable<TrainingHelper.Models.Bay>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Here is a list of all Bays in the Fab.</h4>
<hr />

@foreach (var x in Model)
{
    <div class="row">
        @Html.ActionLink(x.Name, "Details", "Bay", new { id = x.BayId }, new { @class = "btn btn-success custom" })
    </div>
}
