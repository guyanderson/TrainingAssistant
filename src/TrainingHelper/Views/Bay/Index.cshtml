@model IEnumerable<TrainingHelper.Models.Bay>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Here is a list of all Bays in the Fab.</h4>
<hr />

<div class="row">
    <ul>
        @foreach (var x in Model)
        {
            <li>
                <div class="row">
                    <button type="button" class="btn btn-success custom">@Html.DisplayFor(modelItem => x.Name)</button>
                </div>
            </li>
        }
    </ul>
</div>
