@model IEnumerable<TrainingHelper.Models.Area>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Here is a list of all Areas in the Fab.</h4>
<hr />

<div class="row">
    <ul>
        @foreach (var x in Model)
        {
            <div class="row">
                <div class="panel panel-success custom">
                    <div class="panel-heading custom">@Html.DisplayFor(modelItem => x.Name)</div>
                </div>
            </div>
        }
    </ul>
</div>
