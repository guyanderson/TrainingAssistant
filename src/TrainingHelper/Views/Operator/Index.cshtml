@model IEnumerable<TrainingHelper.Models.Operator>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Here is a list of all Operators in the Fab.</h4>
<hr />

<div class="row">
    <ul>
        @foreach (var x in Model)
        {
            <div class="row">
                <div class="panel panel-success">
                    <div class="panel-heading">@Html.DisplayFor(modelItem => x.Name)</div>
                </div>
            </div>
        }
    </ul>
</div>