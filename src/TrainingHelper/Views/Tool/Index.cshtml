@model IEnumerable<TrainingHelper.Models.Tool>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h4>Here is a list of all Tools in the Fab.</h4>
<hr />


@foreach (var x in Model)
{
    <div class="row">
        @Html.ActionLink(x.Name, "Details", "Tool", new { id = x.ToolId }, new { @class = "btn btn-success custom" })
    </div>
}
