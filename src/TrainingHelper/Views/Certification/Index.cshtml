@model IEnumerable<TrainingHelper.Models.Certification>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<h4>Here is a list of all Certifications in the Fab.</h4>
<hr />

@foreach (var x in Model)
{
    <div class="row">
        @Html.ActionLink(x.Name, "Details", "Certification", new { id = x.CertificationId }, new { @class = "btn btn-success custom" })
    </div>
}

